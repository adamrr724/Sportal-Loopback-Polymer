{
  "env": "development",
  "config": {},
  "dataSources": {
    "local": {
      "connector": "memory",
      "localStorage": "loopback-polymer-starter-kit"
    },
    "remote": {
      "connector": "remote",
      "url": "http://localhost:3000/api"
    }
  },
  "models": [
    {
      "name": "LocalTodo",
      "config": {
