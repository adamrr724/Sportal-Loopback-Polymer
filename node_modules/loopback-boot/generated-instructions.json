      },
      "definition": {
        "name": "RemoteTodo",
        "base": "Todo",
        "plural": "Todos",
        "trackChanges": false,
        "enableRemoteReplication": true
      }
    },
    {
      "name": "user",
      "config": {
        "dataSource": "remote"
      }
    }
  ],
  "components": [],
  "mixins": [],
  "files": {
    "boot": [
      "loopback-boot#boot#replication.js"
    ]
